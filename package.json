{
  "name": "@makefilm/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "build:web": "nx run web:build",
    "fixtures": "ts-node database/src/fixtures/index.ts",
    "format": "npm run format:package && nx format:write",
    "format:package": "sort-package-json",
    "lint": "nx run-many -t lint",
    "lint:commit": "commitlint -e",
    "migration:create": "npm run typeorm -- migration:create database/src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- --dataSource=database/src/data-source.ts migration:generate database/src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- --dataSource=database/src/data-source.ts migration:revert",
    "migration:run": "npm run typeorm -- --dataSource=database/src/data-source.ts migration:run",
    "parse": "nx run wiki-scraper:parse",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "start": "nx run-many -t serve",
    "start:api": "nx run api:serve",
    "start:api:e2e": "cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@localhost:5433/poll_db?schema=public\" nx run api:serve",
    "start:web": "nx run web:serve",
    "test": "nx run-many -t test",
    "test:api": "nx run api:test",
    "test:api:e2e": "env-cmd cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@localhost:5433/poll_db?schema=public\" nx run api-e2e:e2e",
    "test:web:e2e": "env-cmd cross-env LOG_LEVEL=\"fatal\" cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@localhost:5433/poll_db?schema=public\" npx nx run web-e2e:e2e",
    "typeorm": "ts-node --project ./tsconfig.typeorm.json ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@loadable/component": "^5.16.4",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-flat-formatter": "^5.0.1",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "logrock": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.3",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "typeor": "^1.2.0",
    "typeorm": "0.3.20",
    "uuid": "^11.0.5"
  },
