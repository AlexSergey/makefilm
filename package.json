{
  "name": "@makefilm/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "format": "npm run format:package && nx format:write",
    "format:package": "sort-package-json",
    "lint": "nx run-many -t lint",
    "lint:commit": "commitlint -e",
    "migration:create": "npm run typeorm -- migration:create ./apps/api/src/common/database/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- --dataSource=apps/api/src/common/database/data-source.ts migration:generate ./apps/api/src/common/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- --dataSource=apps/api/src/common/database/data-source.ts migration:revert",
    "migration:run": "npm run typeorm -- --dataSource=apps/api/src/common/database/data-source.ts migration:run",
    "migration:run:e2e": "npm run typeorm:e2e -- --dataSource=apps/api/src/common/database/data-source.ts migration:run",
    "migration:run:e2e:ci": "npm run typeorm:e2e:ci -- --dataSource=apps/api/src/common/database/data-source.ts migration:run",
    "parse": "nx run wiki-scraper:parse",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "start": "npm run typeorm:linker && nx run-many -t serve",
    "start:api": "npm run typeorm:linker && nx run api:serve",
    "test": "nx run-many -t test",
    "test:api": "nx run api:test",
    "test:api:e2e": "env-cmd cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@localhost:5433/poll_db?schema=public\" npx nx run api-e2e:e2e",
    "test:api:e2e:ci": "env-cmd cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@makefilm_db_tests:5432/poll_db?schema=public\" npx nx run api-e2e:e2e",
    "typeorm": "env-cmd ts-node --project ./tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "typeorm:e2e": "env-cmd cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@localhost:5433/poll_db?schema=public\" ts-node --project tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "typeorm:e2e:ci": "env-cmd cross-env DATABASE_URL=\"postgresql://poll_user:poll_password@makefilm_db_tests:5432/poll_db?schema=public\" ts-node --project tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "typeorm:linker": "ts-node --pretty --project=tsconfig.typeorm.json tools/typeorm-linker/index.ts",
    "typeorm:links": "ts-node --pretty --project=tsconfig.typeorm.json tools/index.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-flat-formatter": "^5.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "pg": "^8.13.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "typeor": "^1.2.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.8.0",
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nrwl/nest": "^19.8.4",
    "@nx/devkit": "20.3.1",
    "@nx/eslint": "20.3.1",
    "@nx/eslint-plugin": "20.3.1",
    "@nx/jest": "20.3.1",
    "@nx/js": "20.3.1",
    "@nx/nest": "20.3.1",
    "@nx/node": "20.3.1",
    "@nx/playwright": "20.3.1",
    "@nx/react": "^20.1.4",
    "@nx/vite": "20.3.1",
    "@nx/web": "20.3.1",
    "@nx/webpack": "20.3.1",
    "@nx/workspace": "20.3.1",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
