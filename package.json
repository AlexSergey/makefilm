{
  "name": "project-name",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "build": "nest build",
    "format": "npm run format:package && npm run format:prettier && npm run format:code",
    "format:code": "eslint . --fix",
    "format:package": "sort-package-json",
    "format:prettier": "prettier --write \"src/**/*.{ts,json}\"",
    "lint": "npm run lint:ts && npm run lint:code",
    "lint:code": "eslint .",
    "lint:commit": "commitlint -e",
    "lint:ts": "tsc --noEmit",
    "migration:create": "npm run typeorm -- migration:create ./src/common/database/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- --dataSource=src/common/database/data-source.ts migration:generate ./src/common/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- --dataSource=src/common/database/data-source.ts migration:revert",
    "migration:run": "npm run typeorm -- --dataSource=src/common/database/data-source.ts migration:run",
    "migration:run:e2e": "npm run typeorm:e2e -- --dataSource=src/common/database/data-source.ts migration:run",
    "migration:run:e2e:ci": "npm run typeorm:e2e:ci -- --dataSource=src/common/database/data-source.ts migration:run",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "schema:drop": "npm run typeorm -- --dataSource=src/common/database/data-source.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/common/database/seeds/run-seed.ts",
    "seed:run:fake": "ts-node -r tsconfig-paths/register ./src/common/database/seeds/run-seed.ts --fake=true",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "env-cmd -f .env.tests jest --verbose",
    "test:cov": "env-cmd -f .env.tests jest --coverage",
    "test:debug": "env-cmd -f .env.tests node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f .env.tests jest --config ./test/jest-e2e.json --verbose",
    "test:e2e:ci": "env-cmd -f .env.ci jest --config ./test/jest-e2e.json --verbose",
    "test:watch": "env-cmd -f .env.tests jest --watch",
    "typeorm": "env-cmd ts-node --project ./tsconfig.json ./node_modules/typeorm/cli.js",
    "typeorm:e2e": "env-cmd -f .env.tests ts-node --project ./tsconfig.json ./node_modules/typeorm/cli.js",
    "typeorm:e2e:ci": "env-cmd -f .env.ci ts-node --project ./tsconfig.json ./node_modules/typeorm/cli.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "pg": "^8.13.1",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "typeor": "^1.2.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-perfectionist": "^4.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "sort-package-json": "^2.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.19.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
